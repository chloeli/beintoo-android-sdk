<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/myScroll" android:layout_width="fill_parent"
	android:scrollbars="vertical" android:layout_height="fill_parent"
	android:fillViewport="true">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/maincontainer"
		android:orientation="vertical" android:background="#d6dbde">
		<LinearLayout android:id="@+id/linearLayout1"
			android:layout_alignParentLeft="true"
			android:layout_height="wrap_content" android:gravity="center" android:layout_width="fill_parent">
			<RelativeLayout
				android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent" android:background="@drawable/bpix">
				<ImageView android:layout_height="wrap_content"
					android:layout_centerHorizontal="true" android:id="@+id/beintoobar"
					android:src="@drawable/beintoobar" android:layout_width="wrap_content"/>
				<Button android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/close"
					android:layout_alignParentRight="true" android:background="@drawable/close" android:layout_centerVertical="true" android:layout_marginRight="8dip"/>
			</RelativeLayout>
		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout1"
			android:layout_width="wrap_content" android:layout_alignParentLeft="true"
			android:layout_height="wrap_content" android:layout_gravity="center"
			android:layout_marginTop="10dip">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/banner"
				android:background="@drawable/boxchallenges"></ImageView>

		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout2"
			android:layout_width="fill_parent" android:padding="0dip"
			android:layout_height="wrap_content">
			<ImageView android:id="@+id/imageView1"
				android:layout_height="wrap_content" android:background="@drawable/layoutbottom"
				android:layout_marginLeft="18dip" android:layout_marginRight="18dip"
				android:layout_marginTop="10dip" android:layout_width="fill_parent"></ImageView>
		</LinearLayout>

		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:id="@+id/goodcontent"
			android:layout_height="wrap_content" android:layout_marginLeft="18dip"
			android:layout_marginRight="18dip" android:background="#FFFFFF"
			android:layout_marginTop="0dip">
			<TableLayout android:id="@+id/usersTableLayout"
				android:layout_height="fill_parent" android:scrollbars="vertical"
				android:background="#FFFFFF" android:stretchColumns="1,*"
				android:layout_width="290dip">

			</TableLayout>

		</LinearLayout>
		<LinearLayout android:layout_height="wrap_content"
			android:id="@+id/linearLayout3" android:layout_width="fill_parent">
			<ImageView android:id="@+id/imageView2"
				android:layout_height="wrap_content" android:background="@drawable/boxombr"
				android:layout_marginLeft="18dip" android:layout_marginRight="18dip"
				android:layout_marginTop="0dip" android:layout_marginBottom="15dip"
				android:layout_width="fill_parent"></ImageView>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:id="@+id/goodcontent"
			android:layout_height="wrap_content" android:gravity="center">
			<Button android:layout_height="wrap_content" android:id="@+id/anotheracc"
				android:background="@drawable/useanother" android:layout_span="2"
				android:layout_width="wrap_content"/>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:layout_height="fill_parent"
			android:gravity="bottom|center" android:id="@+id/fond">
			<ImageView android:layout_height="wrap_content" android:id="@+id/layoutbottom"
				android:layout_width="fill_parent" android:background="@drawable/layoutbottom" />

		</LinearLayout>

	</LinearLayout>
</ScrollView>
				