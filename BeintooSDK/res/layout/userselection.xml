<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/myScroll" android:layout_width="fill_parent"
	android:scrollbars="vertical" android:layout_height="fill_parent"
	android:fillViewport="true">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/maincontainer"
		android:orientation="vertical" android:background="#dddddd">
		<include layout="@layout/beintoobar" />
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="0.75dip" android:background="#FFFFFF" android:id="@+id/whiteline"></LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:id="@+id/textlayout" android:paddingLeft="15dip" android:gravity="center_vertical" android:layout_height="60dip">
			<TextView android:id="@+id/textView1" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:typeface="sans"
				android:textSize="16dip" android:textColor="#333133" android:text="@string/boxUserSel"></TextView>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="0.75dip" android:background="#8F9193" android:id="@+id/grayline"></LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="0.75dip" android:background="#FFFFFF" android:id="@+id/whiteline"></LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:id="@+id/goodcontent"
			android:layout_height="wrap_content" android:layout_marginTop="0dip">
			<TableLayout android:id="@+id/usersTableLayout"
				android:layout_height="fill_parent" android:scrollbars="vertical"
				android:stretchColumns="1" android:layout_width="fill_parent">

			</TableLayout>

		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:id="@+id/goodcontent"
			android:layout_height="wrap_content" android:gravity="center">
			<Button android:id="@+id/anotheracc" android:layout_span="2"
				android:text="@string/anotheraccount" android:textColor="#FFFFFF"
				android:layout_height="50dip" android:layout_width="294dip"
				android:layout_marginTop="11dip" android:textSize="18dip"
				android:layout_marginBottom="11dip" />
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:layout_height="fill_parent"
			android:gravity="bottom|center" android:id="@+id/fond">
			<ImageView android:id="@+id/layoutbottom"
				android:layout_width="fill_parent" android:background="@drawable/footer"
				android:layout_height="11dip" />

		</LinearLayout>

	</LinearLayout>
</ScrollView>
				