<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/myScroll" android:layout_width="fill_parent"
		android:scrollbars="vertical" android:layout_height="fill_parent"
		android:fillViewport="true">
	
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:background="#d9dde2">
	<ImageView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:src="@drawable/layoutbottom"
		android:layout_alignParentTop="true" android:id="@+id/layouttop">
	</ImageView>
	<RelativeLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:orientation="horizontal"
		android:background="#d9dde2" android:id="@+id/main"
		android:layout_marginTop="9dip">
		<ImageView android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:id="@+id/logotry"
			android:src="@drawable/logobtn" android:layout_centerHorizontal="true"
			android:paddingTop="10dip"></ImageView>
		<ImageView android:layout_height="wrap_content"
			android:layout_centerHorizontal="true" android:layout_width="wrap_content"
			android:id="@+id/trymessage" android:src="@drawable/trymessage"
			android:layout_marginTop="50dip"></ImageView>
			
			<Button android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/trybt" android:background="@drawable/trybutton" android:layout_centerHorizontal="true" android:layout_marginTop="230dip"></Button>
			<Button android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/nothanksbt" android:background="@drawable/nothanksbt" android:layout_centerHorizontal="true" android:layout_marginTop="320dip"></Button>
	</RelativeLayout>


	<ImageView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:id="@+id/layoutbottom"
		android:src="@drawable/layoutbottom" android:layout_alignParentBottom="true">
	</ImageView>
	
</RelativeLayout>

</ScrollView>
