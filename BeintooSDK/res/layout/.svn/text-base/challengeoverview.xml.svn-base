<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/myScroll" android:layout_width="fill_parent"
	android:scrollbars="vertical" android:layout_height="fill_parent"
	android:fillViewport="true">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/maincontainer"
		android:orientation="vertical" android:background="#d6dbde">
		<LinearLayout android:id="@+id/linearLayout2"
			android:layout_width="fill_parent" android:padding="0dip"
			android:layout_height="wrap_content">
			<ImageView android:id="@+id/imageView1"
				android:layout_height="wrap_content" android:background="@drawable/layoutbottom"
				android:layout_marginLeft="18dip" android:layout_marginRight="18dip"
				android:layout_marginTop="10dip" android:layout_width="fill_parent"></ImageView>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:id="@+id/goodcontent"
			android:layout_marginLeft="18dip" android:layout_marginRight="18dip"
			android:background="#FFFFFF" android:layout_height="wrap_content">
			<LinearLayout android:id="@+id/linearLayout1"
				android:layout_width="wrap_content" android:layout_alignParentLeft="true"
				android:layout_height="wrap_content" android:layout_gravity="center"
				android:layout_marginTop="10dip" android:background="@drawable/bar"
				android:gravity="center_vertical|center_horizontal">
				<ImageView android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/leaderbar"
					android:layout_marginTop="0dip"></ImageView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="#FFFFFF"
					android:textStyle="bold" android:textSize="20dip" android:text="contest"
					android:id="@+id/contname"></TextView>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout android:id="@+id/content"
			android:layout_height="wrap_content" android:layout_marginLeft="18dip"
			android:layout_marginRight="18dip" android:layout_width="fill_parent"
			android:background="#FFFFFF" android:padding="5dip"
			android:orientation="vertical">
			<LinearLayout android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/nicks"
				android:orientation="vertical">
				<LinearLayout android:gravity="center_vertical"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:orientation="horizontal" android:padding="10dip"
					android:id="@+id/from1ll">
					<LinearLayout android:layout_width="wrap_content" android:id="@+id/frompict" android:layout_height="wrap_content" android:minHeight="70dip" android:minWidth="70dip">
					</LinearLayout>
					<LinearLayout android:id="@+id/linearLayout7" android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent" android:layout_marginLeft="15dip">
						<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:textStyle="bold" android:text="TextView" android:textColor="#83be56" android:id="@+id/fromnick"></TextView>
						<TextView android:text="TextView" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/scorefrom" android:textColor="#5E5A5B"></TextView>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:orientation="horizontal"
					android:gravity="center_vertical" android:padding="10dip"
					android:id="@+id/from2ll">
					<LinearLayout android:layout_width="wrap_content" android:id="@+id/topict" android:layout_height="wrap_content" android:minHeight="70dip" android:minWidth="70dip">
					</LinearLayout>
					<LinearLayout android:id="@+id/linearLayout8" android:layout_height="wrap_content" android:layout_marginLeft="15dip" android:orientation="vertical" android:layout_width="fill_parent">
						<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:textStyle="bold" android:text="TextView" android:textColor="#83be56" android:id="@+id/tonick"></TextView>
						<TextView android:text="TextView" android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/scoreto" android:textColor="#5E5A5B"></TextView>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_marginLeft="10dip" android:id="@+id/linearLayout6">
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Status:" android:id="@+id/textView1" android:textColor="#5E5A5B" android:textStyle="bold"></TextView>
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10dip" android:text="TextView" android:id="@+id/status" android:textColor="#83be56"></TextView>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/datesll"
				android:orientation="vertical" android:paddingLeft="10dip" android:layout_marginTop="10dip">
				<LinearLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/linearLayout4">
					<TextView android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:text="Start date:"
						android:id="@+id/starttit" android:textColor="#5E5A5B" android:textStyle="bold"></TextView>
					<TextView android:layout_height="wrap_content"
						android:id="@+id/startdate" android:layout_marginLeft="15dip" android:layout_width="fill_parent" android:text="textview" android:textColor="#83be56"></TextView>
				</LinearLayout>
				<LinearLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/linearLayout5">
					<TextView android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/endtit"
						android:text="End date:" android:textStyle="bold" android:textColor="#5E5A5B"></TextView>
					<TextView android:layout_height="wrap_content"
						android:text="TextView"
						android:layout_marginLeft="15dip" android:id="@+id/enddate" android:layout_width="fill_parent" android:textColor="#83be56"></TextView>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/winll"
				android:paddingLeft="10dip" android:layout_marginTop="10dip"
				android:orientation="vertical">
				<LinearLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/LinearLayout02">
					<TextView android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/wintit"
						android:text="Winner:" android:textColor="#5E5A5B" android:textStyle="bold"></TextView>
					<TextView android:layout_height="wrap_content"
						android:layout_marginLeft="15dip"
						android:text="TextView" android:id="@+id/winner" android:layout_width="fill_parent" android:textColor="#83be56"></TextView>
				</LinearLayout>
				<LinearLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/LinearLayout01">
					<TextView android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/prizetit"
						android:text="Prize:" android:textColor="#5E5A5B" android:textStyle="bold"></TextView>
					<TextView android:layout_height="wrap_content"
						android:layout_marginLeft="15dip"
						android:text="TextView" android:id="@+id/prize" android:layout_width="fill_parent" android:textColor="#83be56"></TextView>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout android:layout_height="wrap_content"
			android:id="@+id/linearLayout3" android:layout_width="fill_parent">
			<ImageView android:id="@+id/imageView2"
				android:layout_height="wrap_content" android:background="@drawable/boxombr"
				android:layout_marginLeft="18dip" android:layout_marginRight="18dip"
				android:layout_marginTop="0dip" android:layout_marginBottom="15dip"
				android:layout_width="fill_parent"></ImageView>
		</LinearLayout>
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:layout_height="fill_parent"
			android:gravity="bottom|center" android:id="@+id/fond">
			<ImageView android:layout_height="wrap_content" android:id="@+id/layoutbottom"
				android:layout_width="fill_parent" android:background="@drawable/layoutbottom" />

		</LinearLayout>
	</LinearLayout>
</ScrollView>
