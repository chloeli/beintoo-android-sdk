<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:id="@+id/maincontainer" android:orientation="vertical"
	android:layout_height="fill_parent" android:background="#dddddd"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#8F9193"
		android:id="@+id/grayline"></LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#FFFFFF"
		android:id="@+id/whiteline"></LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:id="@+id/you" android:orientation="horizontal"
		android:layout_height="wrap_content" android:background="#dddddd"
		android:paddingBottom="5dip" android:paddingLeft="10dip"
		android:paddingRight="5dip" android:paddingTop="5dip">
		<ImageView android:id="@+id/fromimg" android:layout_width="50dip"
			android:layout_height="50dip" android:layout_marginRight="10dip" />
		<LinearLayout android:id="@+id/linearLayout1"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:orientation="vertical" android:layout_gravity="center">
			<TextView android:id="@+id/fromname" android:layout_width="wrap_content"
				android:textStyle="bold" android:layout_height="wrap_content"
				android:text="TextView" android:textColor="#000000" />
			<TextView android:id="@+id/fromwinner" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:textStyle="bold"
				android:layout_height="wrap_content" android:text="@string/challWinner"
				android:visibility="gone" />
			<TextView android:id="@+id/frompoints" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content"
				android:text="TextView" android:visibility="gone" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#8F9193"
		android:id="@+id/grayline"></LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#FFFFFF"
		android:id="@+id/whiteline"></LinearLayout>
	<LinearLayout android:id="@+id/details"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:orientation="horizontal" android:padding="10dip">
		<RelativeLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_gravity="center">
			<ImageView android:id="@+id/imageView2"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:src="@drawable/challbubblev"/>
			<TextView android:id="@+id/bigtext" android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true" android:text="VS"
				android:textColor="#FFFFFF" />
		</RelativeLayout>
		<LinearLayout android:id="@+id/linearLayout2"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:layout_gravity="center_vertical" android:layout_marginLeft="10dip"
			android:orientation="vertical">
			<TextView android:id="@+id/contest" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content" />
			<TextView android:id="@+id/status" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content"
				android:visibility="gone" />
			<TextView android:id="@+id/amountbet" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content" />
			<TextView android:id="@+id/amountwin" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content" />
			<TextView android:id="@+id/goalpoints" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content" />
			<TextView android:id="@+id/startdate" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content"
				android:visibility="gone" />
			<TextView android:id="@+id/enddate" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content"
				android:visibility="gone" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#8F9193"
		android:id="@+id/grayline"></LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#FFFFFF"
		android:id="@+id/whiteline"></LinearLayout>
	<LinearLayout android:id="@+id/other"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="#dddddd" android:orientation="horizontal"
		android:paddingLeft="10dip" android:paddingBottom="5dip"
		android:paddingRight="5dip" android:paddingTop="5dip">
		<ImageView android:id="@+id/toimg" android:layout_width="50dip"
			android:layout_height="50dip" android:layout_marginRight="10dip" />
		<LinearLayout android:id="@+id/LinearLayout02"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:orientation="vertical" android:layout_gravity="center">
			<TextView android:id="@+id/toname" android:layout_width="wrap_content"
				android:textStyle="bold" android:layout_height="wrap_content"
				android:text="TextView" android:textColor="#000000" />
			<TextView android:id="@+id/towinner" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:textStyle="bold"
				android:layout_height="wrap_content" android:text="@string/challWinner"
				android:visibility="gone" />
			<TextView android:id="@+id/topoints" android:layout_width="wrap_content"
				android:textColor="#6a6a6a" android:layout_height="wrap_content"
				android:text="TextView" android:visibility="gone" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#8F9193"
		android:id="@+id/grayline"></LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0.75dip" android:background="#FFFFFF"
		android:id="@+id/whiteline"></LinearLayout>
	<LinearLayout android:id="@+id/buttons"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:gravity="center" android:layout_margin="10dip"
		android:visibility="gone">
		<Button android:id="@+id/refuse" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="@string/alliancerefuse"
			android:textColor="#FFFFFF" android:layout_marginRight="2dip"
			android:layout_weight="1" />
		<Button android:id="@+id/accept" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="@string/allianceaccept"
			android:textColor="#FFFFFF" android:layout_marginLeft="2dip"
			android:layout_weight="1" />
	</LinearLayout>

</LinearLayout>
